<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="io.techybrain.minagoi" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>MINAGOI</name>
  <description>A FREE App to master Japanese listening skills</description>
  <author email="canhnht1709@gmail.com" href="http://toithacmac.wordpress.com">Techybrain Team</author>
  <content src="index.html"/>
  <access origin="*"/>
  <access launch-external="yes" origin="mailto:*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <allow-intent href="market:*"/>
    <icon src="resources/android/icon/drawable-ldpi-icon-v1.png" density="ldpi"/>
    <icon src="resources/android/icon/drawable-mdpi-icon-v1.png" density="mdpi"/>
    <icon src="resources/android/icon/drawable-hdpi-icon-v1.png" density="hdpi"/>
    <icon src="resources/android/icon/drawable-xhdpi-icon-v1.png" density="xhdpi"/>
    <icon src="resources/android/icon/drawable-xxhdpi-icon-v1.png" density="xxhdpi"/>
    <icon src="resources/android/icon/drawable-xxxhdpi-icon-v1.png" density="xxxhdpi"/>
    <splash src="resources/android/splash/splash.png" density="port-ldpi"/>
    <splash src="resources/android/splash/splash.png" density="port-mdpi"/>
    <splash src="resources/android/splash/splash.png" density="port-hdpi"/>
    <splash src="resources/android/splash/splash.png" density="port-xhdpi"/>
    <splash src="resources/android/splash/splash.png" density="port-xxhdpi"/>
    <splash src="resources/android/splash/splash.png" density="port-xxxhdpi"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="android-minSdkVersion" value="16"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="SplashMaintainAspectRatio" value="true"/>
  <preference name="FadeSplashScreenDuration" value="300"/>
  <preference name="SplashScreen" value="screen"/>
  <preference name="SplashScreenDelay" value="7000"/>
  <preference name="ShowSplashScreenSpinner" value="true"/>
  <preference name="AutoHideSplashScreen" value="false"/>
  <feature name="StatusBar">
    <param name="ios-package" onload="true" value="CDVStatusBar"/>
  </feature>
  <plugin name="ionic-plugin-keyboard" spec="~2.2.1"/>
  <plugin name="cordova-plugin-whitelist" spec="1.3.1"/>
  <plugin name="cordova-plugin-console" spec="1.0.5"/>
  <plugin name="cordova-plugin-statusbar" spec="2.2.1"/>
  <plugin name="cordova-plugin-device" spec="1.1.4"/>
  <plugin name="cordova-plugin-media" spec="~2.4.1"/>
  <plugin name="cordova-plugin-file" spec="~4.3.1"/>
  <plugin name="cordova-plugin-x-toast" spec="~2.5.2"/>
  <plugin name="cordova-sqlite-storage" spec="~2.0.0"/>
  <plugin name="cordova-plugin-file-transfer" spec="~1.6.1"/>
  <plugin name="cordova-plugin-spinner-dialog" spec="~1.3.1"/>
  <plugin name="cordova-plugin-network-information" spec="~1.3.1"/>
  <plugin name="cordova-plugin-tts" spec="~0.2.3"/>
  <plugin name="cordova-plugin-facebook4" spec="~1.7.4">
    <variable name="APP_ID" value="1807313106195382"/>
    <variable name="APP_NAME" value="MINAGOI - dev"/>
  </plugin>
  <plugin name="cordova-plugin-nativeaudio" spec="~3.0.7"/>
  <plugin name="cordova-plugin-splashscreen" spec="~4.0.1"/>
  <icon src="resources/android/icon/drawable-xhdpi-icon.png"/>
</widget>
